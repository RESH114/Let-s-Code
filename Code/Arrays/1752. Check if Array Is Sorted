public class Solution {
    public static int isSorted(int n, int []nums) {
        // Write your code here.
         int sorted = 1;
        for(int i=0;i<nums.length-1;i++){
            if(nums[i] > nums[i+1]){
                sorted = 0;
                break;
            }
        }
        return sorted;
    }
}
