/*

*/
import java.util.*;
class Main {
    public static void main(String[] args) {
        int[][] twoDArray = {
            {1, 2, 3},
            {4, 5, 6},
            {7, 8, 9}
        };
        int row = twoDArray.length;
        int col = twoDArray[0].length;
        int oneDArray[] = new int[row*col];
        int ind=0;
        for(int i=0;i<row;i++){
            for(int j=0;j<col;j++){
                oneDArray[ind++] = twoDArray[i][j];
            }
        }
        System.out.println(Arrays.toString(oneDArray));
    }
    
}
