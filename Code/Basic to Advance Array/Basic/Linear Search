
public class Main
{
	public static void main(String[] args) {
		int arr[] = {10, 20, 30, 40, 50};
		int k = 30;
		int n = arr.length-1;
		System.out.println(helper(arr,k,n));
	}
	public static int helper(int arr[],int k,int n){
	    for(int i=0;i<n;i++){
	        if(arr[i] == k){
	            return i+1;
	        }
	    }
	    return -1;
	}
}
